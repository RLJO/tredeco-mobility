<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data >

    <record model="ir.module.category" id="module_category_repair_management">
            <field name="name">Repair</field>
            <field name="description">Helps you manage your Repairs.</field>
            <field name="sequence">200</field>
    </record>
    <record id="group_repair_viewer" model="res.groups">
        <field name="name">Viewer</field>
        <field name="category_id" ref="tredeco_access_writes.module_category_repair_management"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="group_repair_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="tredeco_access_writes.module_category_repair_management"/>
        <field name="implied_ids" eval="[(4, ref('tredeco_access_writes.group_repair_viewer'))]"/>
    </record>
    <record id="group_repair_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="tredeco_access_writes.module_category_repair_management"/>
        <field name="implied_ids" eval="[(4, ref('tredeco_access_writes.group_repair_user'))]"/>
    </record>

</data>
</odoo>